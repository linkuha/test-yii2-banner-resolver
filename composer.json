{
    "name": "linkuha/yii2-banner-resolver",
    "description": "Banner Resolver based on region, probability and query parameters.",
    "type": "yii2-extension",
    "keywords": ["yii2", "yii 2", "banners"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Alexandr Pudich",
            "email": "linkuha@gmail.com",
            "homepage": "https://pudich.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/linkuha/yii2-banner-resolver/issues?state=open",
        "source": "https://github.com/ElisDN/yii2-banner-resolver"
    },
    "require": {
        "php": "^7.0",
        "yiisoft/yii2": ">=2.0.14",
        "bower-asset/jquery.inputmask": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "7.*"
    },
    "autoload": {
        "psr-4": {
            "linkuha\\BannerResolver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "linkuha\\BannerResolver\\tests\\": "tests/"
        }
    },
    "config": {
        "fxp-asset": {
            "installer-paths": {
                "npm-asset-library": "vendor/npm",
                "bower-asset-library": "vendor/bower"
            },
            "repositories": [
                {
                    "type": "bower-vcs",
                    "url": "https://github.com/RobinHerbots/Inputmask.git",
                    "name": "bower-asset/jquery.inputmask"
                }
            ]
        }
    }
}
